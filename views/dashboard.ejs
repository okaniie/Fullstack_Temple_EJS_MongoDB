                        <a href="profile.html"><img src="imgs/profile-img.png" alt=""></a>
                    </div>
                    <div class="user-2">
                        <a href="profile.html"></a>
                        <div class="valid">Current Investment:<span><%= user.Deposits %></span> </div>
                    </div>
                </div>
                <div class="dashboard">
                    <div class="main">Main</div>
                    <div class="deposit">
                        <div class="deposit-1">
                            <a href="/dashboard" class="show show-1">
                                <i id="sort" class="fas fa-house house-1"></i>
                                <div class="withdraw name">Dashboard</div>
                            </a>
                        </div>
                        <div class="deposit-1">
                            <a href="/deposit" class="show">
                                <i id="sort" class="fa-solid fa-table-cells"></i>
                                <div class="withdraw">Deposit</div>
                            </a>
                        </div>
                        <div class="deposit-1">
                            <a href="/withdraw" class="show">
                                <i id="sort" class="fa-solid fa-book"></i>
                                <div class="withdraw">withdraw</div>
                            </a>
                        </div>
                        <div class="deposit-1">
                            <a href="/experts" class="show">
                                <i  id="sort" class="fa-solid fa-user"></i>
                                <div class="withdraw">Follow Experts Traders</div>
                            </a>
                        </div>
                        <div class="deposit-1">
                            <a href="invsetment.html" class="show">
                                <i id="sort" class="fas fa-table-list"></i>
                                <div class="withdraw">Investment</div>
                            </a>
                        </div>
                        <div class="deposit-1" id="wrapper">
                            <div class="show">
                                <i id="sort" class="fa-solid fa-wallet"></i>
                                <div class="supp">
                                    <div class="withdraw">Investment plans</div>
                                    <i class="uil uil-angle-down down"></i>
                                </div>
                            </div>
                            <ul class="option">
                                <li><a href="/plans">Basic</a></li>
                                <li><a href="/plans">Silver</a></li>
                                <li><a href="/plans">Gold</a></li>
                                <li><a href="/plans">Platinum</a></li>
                                <li><a href="/plans">Diamond</a></li>
                                <li><a href="/plans">Sapphire</a></li>
                            </ul>
                        </div>
                        <div class="deposit-1">
                            <a href="/wallet" class="show">
                                <i id="sort" class="fa-brands fa-google-wallet"></i>
                                <div class="withdraw">Connet Your Wallet</div>
                            </a>
                        </div>
                        <div class="deposit-1">
                            <a href="#" class="show">
                                <i id="sort" class="fa-solid fa-right-from-bracket"></i>
                                <div class="withdraw">Log Out</div>
                            </a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
