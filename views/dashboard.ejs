<body>
    <!-- HEADER -->
    <header>
        <div class="cointainer">
            <div class="navbar">
                <div class="coint">
                    <label for="show-menu" class="menu-icon"><i class="fas fa-bars"></i></label >
                    <div class="crypto">
                        <a href="/">
                            <div class="images">
                                <img src="imgs/yellow-logo.png" alt="">
                            </div>   
                            <div class="crypto-1">CRYPTO<span>FX</span>MGMT</div> 
